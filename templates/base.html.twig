<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block modal %}{% endblock %}
        <div class="d-flex flex-row min-vh-100">
            <div class="d-flex flex-column flex-shrink-0 bg-white shadow-lg" style="width:260px;">
                {% include 'sidebar/sidebar.html.twig' %}
            </div>
            <div class="o-bg-gray w-100">
                <nav class="bg-white p-3 mb-3">
                    <div class="pb-3">
                        <span class="font-weight-bold ml-2 h3 pb-3"></span> 
                    </div>
                </nav>
                {% block body %}{% endblock %}
            </div>
        </div>
        {{ flasher_render() }}
    </body>
</html>
